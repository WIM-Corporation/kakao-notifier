  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"apps/**/*.ts\"",
    "lint:fix": "eslint \"apps/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "concurrently \"nest build api\" \"nest build batch\" \"nest build processor\"",
    "start": "nest start",
    "start:dev": "yarn prebuild && nest start --watch",
    "start:debug": "yarn prebuild && nest start --debug --watch",
    "start:all": "concurrently \"nest start --watch api\" \"nest start --watch batch\" \"nest start --debug --watch processor\"",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test": "jest -i --config jest-e2e.json",
    "test:unit": "jest --clearCache && jest --watch",
    "test:watch": "jest --watch -u -i --config jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -i -u",
    "test:e2e": "jest -i -u --forceExit --config jest-e2e.json",
    "test:all": "npm run test && npm run test:e2e",
    "setting": "ts-node ./libs/test-util/src/global-set-up.ts",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.19",
